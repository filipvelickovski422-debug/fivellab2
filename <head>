        <div class="stats-grid">
            <div class="stat-card">
                <div class="stat-number" data-target="1000000" data-format="$M+">0</div>
                <div class="stat-label">Ad Spend Managed</div>
            </div>
            <div class="stat-card">
                <div class="stat-number" data-target="5" data-format="x">0</div>
                <div class="stat-label">Avg ROAS</div>
            </div>
            <div class="stat-card">
                <div class="stat-number" data-target="4.1" data-format="%">0</div>
                <div class="stat-label">Avg CTR</div>
            </div>
            <div class="stat-card">
                <div class="stat-number" data-target="50" data-format="+">0</div>
                <div class="stat-label">Campaigns Launched</div>
            </div>
        </div>
