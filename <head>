<script>
const counters = document.querySelectorAll('.stat-number');

counters.forEach(counter => {
    const target = parseFloat(counter.dataset.target);
    const format = counter.dataset.format || '';
    let count = 0;

    const duration = 1500; // –≤—Ä–µ–º–µ –Ω–∞ –∞–Ω–∏–º–∞—Ü–∏—ò–∞—Ç–∞ –≤–æ ms
    const stepTime = 20; // –∫–æ–ª–∫—É —á–µ—Å—Ç–æ —Å–µ –∞–ø–¥–µ—ò—Ç—É–≤–∞
    const steps = Math.ceil(duration / stepTime);
    const increment = target / steps;

    const updateCounter = () => {
        count += increment;
        if (count >= target) {
            count = target;
            counter.innerText = formatNumber(count, format);
        } else {
            counter.innerText = formatNumber(count, format, true);
            setTimeout(updateCounter, stepTime);
        }
    }

    const formatNumber = (num, format, duringAnimation = false) => {
        if (format === "$M+") {
            return duringAnimation ? Math.floor(num).toLocaleString() : "$" + (num / 1000000).toFixed(1) + "M+";
        } else if (format === "x") {
            return duringAnimation ? num.toFixed(1) : num.toFixed(1) + "x";
        } else if (format === "%") {
            return duringAnimation ? num.toFixed(1) : num.toFixed(1) + "%";
        } else if (format === "+") {
            return duringAnimation ? Math.floor(num) : Math.floor(num) + "+";
        } else {
            return Math.floor(num);
        }
    }

    updateCounter();
});
</script>

